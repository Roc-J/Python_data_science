    "import numpy as np\n",
    "y = np.array([1,0,2,1,1])\n",
    "print y\n",
    "set_y = set(y)\n",
    "for y_lable in set_y:\n",
    "    c = len(np.where(y == y_lable)[0])\n",
    "    print c"
   ]
